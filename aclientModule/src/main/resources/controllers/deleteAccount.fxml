<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="121.0" prefWidth="264.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.DeleteAccountController">
   <children>
      <Label fx:id="deleting" layoutX="67.0" layoutY="23.0" text="Удаление аккаунта">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button layoutX="37.0" layoutY="71.0" mnemonicParsing="false" onAction="#deleteAccount" style="-fx-background-radius: 5; -fx-border-radius: 5; -fx-border-color: #696969;" text="Да, удалить аккаунт" textFill="#008c02">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Label layoutX="96.0" layoutY="43.0" text="Вы уверены?" />
      <Button layoutX="182.0" layoutY="71.0" mnemonicParsing="false" onAction="#noDeleteAccount" style="-fx-background-radius: 5; -fx-border-radius: 5; -fx-border-color: #696969;" text="Нет" textFill="RED">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
